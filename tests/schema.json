{
	"type": "object",
	"properties": {
		"contractAddress": {
            "type": "string",
            "pattern": "^0x[a-fA-F0-9]{40}$"
        },
        "description": {
            "type": "string"
        },
        "chain": {
            "type": "string",
            "enum": ["eth", "polygon", "matic", "bsc"]
        },
        "delistedDate": {
            "type": "string",
            "pattern": "\\d{4}-\\d{2}-\\d{2}"
        },
        "delistedFrom": {
            "type": "string",
            "enum": ["opensea", "rarible"]
        },
        "projectUrl": {
            "type": "string"
        }
	},
	"required": [
        "contractAddress"
    ],
    "additionalProperties": false
}